--player code
function make_player()
	p={}

	p.x=3
	p.y=2

	p.sprite=6

end

function draw_player()
	 spr(p.sprite,p.x*8,p.y*8)

end

function move_player()
	newx = p.x
	newy = p.y
	if (btnp(⬅️)) newx -= 1
		-- shift l
	if (btnp(➡️)) newx += 1
		-- shift r
	if (btnp(⬆️)) newy -= 1
		-- shift u
	if (btnp(⬇️)) newy += 1
		-- shift d

		-- letting using key

	interact(newx,newy)

	if (can_move(newx,newy)) then
		p.x = mid(0, newx, 127)
		p.y = mid(0, newy, 63)
		--sets bounds to stay within map
	end
end

